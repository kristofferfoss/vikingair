<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %><%@ page contentType="text/html;charset=UTF-8" language="java" %><html><head>    <title>Title</title></head><body><div class="wrap">    <div class="container">        <table class="table table-hover">            <thead>            <tr>                <th>                    TicketID                </th>                <th>                    CustomerID                </th>                <th>                    FLightID                </th>                <th>                    Gate                </th>                <th>                    FirtsName                </th>                <th>                    LastName                </th>                <th>                    SeatRow                </th>                <th>                    SeatLetter                </th>                <th>                    ClassType                </th>            </tr>            </thead>            <jsp:useBean id="ticket" scope="request" type="java.util.List"/>            <c:forEach items="${ticket}" var="ticket">                <tr>                    <td>                        <c:out value="${tic.TicketID}"/>                    </td>                    <td>                        <c:out value="${tic.CustomerID}"/>                    </td>                    <td>                        <c:out value="${tic.FlightID}"/>                    </td>                    <td>                        <c:out value="${tic.Gate}"/>                    </td>                    <td>                        <c:out value="${tic.FirstName}"/>                    </td>                    <td>                        <c:out value="${tic.LastName}"/>                    </td>                    <td>                        <c:out value="${tic.SeatRow}"/>                    </td>                    <td>                        <c:out value="${tic.SeatLetter}"/>                    </td>                    <td>                        <c:out value="${tic.ClassType}"/>                    </td>                </tr>            </c:forEach>        </table>    </div></div></body>